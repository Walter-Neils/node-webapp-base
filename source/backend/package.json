{
    "scripts": {
        "build": "rm -rf ./build; mkdir build;tsc -p .",
        "obfuscate:standard": "javascript-obfuscator --string-array true --simplify true --self-defending true --compact false --control-flow-flattening true --identifier-names-generator 'mangled-shuffled' ./build/ --output ./build/obfuscated/",
        "obfuscate:high": "javascript-obfuscator --string-array true --simplify true --self-defending true --compact false --control-flow-flattening true --identifier-names-generator 'hexadecimal' --transform-object-keys true ./build/ --output ./build/obfuscated/",
        "start": "tsc-watch --onSuccess \"node build/entryPoint.js\" | pino-pretty",
        "production": "node build/server/src/entryPoint.js",
        "dev": "tsc-watch --onSuccess \"node --loader ts-loader build/server/src/entryPoint.ts\"",
        "dev:v8-opt": "tsc-watch --onSuccess \"node --inspect --prof --trace-deopt --trace-opt build/server/src/entryPoint.js\"",
        "depcruise": "npx depcruise src --no-config --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg",
        "depgraph": "npx ts_dependency_graph --start ./src/entryPoint.ts"
    },
    "dependencies": {
        "adm-zip": "^0.5.10",
        "async-mutex": "^0.4.0",
        "axios": "^1.4.0",
        "cookie-parser": "^1.4.6",
        "cookie-session": "^2.0.0",
        "cookies": "^0.8.0",
        "express": "^4.18.2",
        "http": "^0.0.1-security",
        "http-proxy-middleware": "^2.0.6",
        "jimp": "^0.22.8",
        "mongodb": "^5.5.0",
        "node-fetch": "^3.3.1",
        "nodemon": "^2.0.22",
        "pino": "^8.15.0",
        "uuid": "^9.0.0",
        "v8-profiler-next": "^1.9.0",
        "web-push": "^3.6.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.2.3",
        "@types/react-dom": "^18.2.7",
        "@types/uuid": "^9.0.2",
        "dependency-cruiser": "^13.1.1",
        "ts_dependency_graph": "^2.0.0",
        "typescript": "^5.1.6",
        "@types/adm-zip": "^0.5.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cookie-session": "^2.0.44",
        "@types/cookies": "^0.7.7",
        "@types/pino": "^7.0.5",
        "@types/web-push": "^3.3.2",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "javascript-obfuscator": "^4.0.2"
    },
    "type": "module"
}